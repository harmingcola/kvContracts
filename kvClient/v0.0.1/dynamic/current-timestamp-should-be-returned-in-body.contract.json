{
  "info" : {
    "details": "The server should return the current timestamp when requested",
    "tags": "timestamp"
  },
  "request" : {
    "method" : "GET",
    "path" : "/kv/service/times",
    "headers" : {
      "source-app" : "kvClient"
    }
  },
  "response" : {
    "status" : 200,
    "body" : "${contract.timestamp}",
    "headers" : {
      "source-app" : "kvServer"
    }
  }
}